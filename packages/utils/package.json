{
  "name": "@app/utils",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run build:compile",
    "build:compile": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "clean": "rm -rf dist",
    "dev": "npm run clean && npm run build:watch",
    "format": "eslint . --max-warnings=0 --fix",
    "lint": "eslint . --max-warnings=0",
    "postinstall": "npm run build",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@app/config": "*"
  },
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "import": "./dist/*.js"
    },
    "./constants": {
      "types": "./src/constants/index.ts",
      "import": "./dist/constants/index.js"
    },
    "./errors": {
      "types": "./src/errors/index.ts",
      "import": "./dist/errors/index.js"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./dist/types/index.js"
    }
  }
}
